    "@angular/platform-server": "^4.0.1",
    "@angular/router": "^4.0.1",
    "@angular/upgrade": "^2",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.2.2",
    "angular-in-memory-web-api": "^0.2.4",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "moment": "^2.18.1",
